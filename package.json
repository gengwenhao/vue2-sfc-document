{
  "name": "vue2-sfc-document",
  "version": "0.4.4",
  "description": "解析 Vue2 SFC 自动生成文档，支持多种文档格式，包括 Markdown、Docx、PDF、HTML 等等。",
  "main": "cli/index.js",
  "scripts": {
    "build": "webpack -c webpack.config.js",
    "test": "jest",
    "commitmsg": "validate-commit-msg",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepare": "husky install"
  },
  "bin": {
    "vue-sfc-document": "./cli/index.js",
    "sfcdoc": "./cli/index.js",
    "sfc2doc": "./cli/index.js"
  },
  "homepage": "https://github.com/gengwenhao/vue2-sfc-document",
  "repository": {
    "url": "https://github.com/gengwenhao/vue2-sfc-document.git"
  },
  "directories": {
    "doc": "doc",
    "example": "examples",
    "test": "test"
  },
  "keywords": [
    "Vue2",
    "SFC",
    "Document"
  ],
  "author": "gengwenhao",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "7.23.0",
    "@babel/core": "7.23.3",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.23.3",
    "@babel/traverse": "^7.23.4",
    "@babel/types": "^7.23.4",
    "@vue/compiler-sfc": "^3.3.9",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "handlebars": "^4.7.8",
    "jimp": "^0.22.10",
    "log-symbols": "^5.1.0",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "conventional-changelog-cli": "^1.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "validate-commit-msg": "^2.11.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
